{
  "name": "nodejs-webpack",
  "version": "1.0.0",
  "main": "src/index.js",
  "scripts": {
    "dev": "nodemon src/index.js",
    "pull": "node git.config.js pull",
    "push": "node git.config.js push",
    "build": "webpack --mode production && javascript-obfuscator dist/bundle.js --output dist/main.js && rm dist/bundle.js",
    "package": "tar -C dist  -cvJf dist/dist.tar.xz main.js",
    "clean": "rm -rf node_modules dist package-lock.json"
  },
  "dependencies": {
    "express": "^4.17.1",
    "javascript-obfuscator": "^2.11.0",
    "nodemon": "^2.0.7",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0"
  }
}
